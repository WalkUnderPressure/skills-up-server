{
    "version": 2,
    "devCommand": "npm run start:dev",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.ts"
        }
    ]
}
